{
  "name": "js-bundler-output-compare",
  "version": "0.0.1",
  "description": "Compare output of different js bundlers",
  "main": "index.js",
  "repository": "https://github.com/foisonocean/js-bundler-output-compare.git",
  "author": "foisonocean",
  "license": "MIT",
  "private": true,
  "scripts": {
    "start": "run-s clean build format",
    "clean": "rimraf output",
    "build": "run-s build:webpack build:webpack:es5 build:webpack:es5:uglify build:webpack:es5:closure build:rollup build:rollup:es5 build:rollup:es5:uglify build:rollup:es5:closure build:parcel:es5 build:parcel build:parcel:es5:minify build:fuse build:fuse:es5 build:fuse:es5:uglify",
    "format": "prettier --write \"output/**/*.js\"",
    "build:webpack": "cross-env NODE_ENV=production webpack --config webpack.config.js",
    "build:webpack:es5": "cross-env NODE_ENV=production BABEL_ENV=es5 webpack --config webpack.config.es5.js",
    "build:webpack:es5:uglify": "cross-env NODE_ENV=production BABEL_ENV=es5 webpack --config webpack.config.es5.uglify.js",
    "build:webpack:es5:closure": "cross-env NODE_ENV=production BABEL_ENV=es5 webpack --config webpack.config.es5.closure.js",
    "build:rollup": "cross-env NODE_ENV=production rollup -c rollup.config.js",
    "build:rollup:es5": "cross-env NODE_ENV=production BABEL_ENV=rollup-es5 rollup -c rollup.config.es5.js",
    "build:rollup:es5:uglify": "cross-env NODE_ENV=production BABEL_ENV=rollup-es5 rollup -c rollup.config.es5.uglify.js",
    "build:rollup:es5:closure": "cross-env NODE_ENV=production BABEL_ENV=rollup-es5 rollup -c rollup.config.es5.closure.js",
    "build:parcel": "cross-env NODE_ENV=production parcel build src/index.js -d output/parcel --no-minify",
    "build:parcel:es5": "cross-env NODE_ENV=production BABEL_ENV=es5 parcel build src/index.es5.js -d output/parcel-es5 --no-minify",
    "build:parcel:es5:minify": "cross-env NODE_ENV=production BABEL_ENV=es5 parcel build src/index.es5.js -d output/parcel-es5-minify",
    "build:fuse": "node fuse",
    "build:fuse:es5": "node fuse.es5",
    "build:fuse:es5:uglify": "node fuse.es5.uglify"
  },
  "devDependencies": {
    "babel-loader": "^7.1.2",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "closure-webpack-plugin": "^0.0.20",
    "cross-env": "^5.1.3",
    "fuse-box": "^3.0.2",
    "npm-run-all": "^4.1.2",
    "parcel-bundler": "^1.4.1",
    "prettier": "^1.9.2",
    "rimraf": "^2.6.2",
    "rollup": "^0.53.3",
    "rollup-plugin-babel": "^3.0.3",
    "rollup-plugin-closure-compiler-js": "^1.0.6",
    "rollup-plugin-commonjs": "^8.2.6",
    "rollup-plugin-node-resolve": "^3.0.1",
    "rollup-plugin-replace": "^2.0.0",
    "rollup-plugin-uglify": "^2.0.1",
    "tslib": "^1.8.1",
    "typescript": "^2.6.2",
    "uglify-js": "^3.3.5",
    "uglifyjs-webpack-plugin": "^1.1.6",
    "webpack": "^3.10.0"
  },
  "browserslist": [
    "last 2 versions",
    "ie 11"
  ]
}
